#include <bits/stdc++.h>

using namespace std;

long long calc_max(long long *arri,long long rest,long long k,long long *arr,long long n){
  long long temp,i;
  if(rest==1)
    return max(k,(k^arri[0]));
  else{
    temp=calc_max(arri+1,rest-1,k,arr,n);
    long long lcnt=64;long long max=temp;
    while(lcnt--){
      for(i=0;i<n;i++)
	if((arr[i]^temp)>max)
	  max=(arr[i]^temp);
      temp=max;
    }
    return max((arri[0]^temp),temp);
  }
}

int main(){
  long long t,n,k,i;
  long long arr[1001];
  scanf("%lld",&t);
  while(t--){
    scanf("%lld",&n);
    scanf("%lld",&k);
    for(i=0;i<n;i++)
      scanf("%lld",&arr[i]);
    printf("%lld\n",calc_max(arr,n,k,arr,n));
  }
  return 0;
}
